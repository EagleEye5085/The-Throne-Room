<div class="bgd-image">
<p align="left" ><%= button_to "Home", '/', method: :get %></p>
<div class="center-horiz">
    <h4> 5 Most Recent Reviews </h4>
        <%@reviews[0..4].each_with_index do |review, index|%>
            <div class="cards2">
            <h5> Review #<%= index + 1 %>:</h5>
            <p> Ambiance: <%= review.ambiance %> </p>
            <p> Toliet Paper Quality: <%= review.tp_quality %> </p>
            <p> Accessibility: <%= review.accessibility %> </p>
            <p> Privacy: <%= review.privacy %> </p>
            <p> Cleanliness: <%= review.cleanliness %> </p>
            <p> Other Comments: </p><p>"<%= review.other_comments %>"</p>
            </div><br>
        <%end%>
    </div>
</div>
